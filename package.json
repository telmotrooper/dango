    "express": "^4.16.4",
    "graphviz": "0.0.9",
    "http-proxy-middleware": "^0.20.0",
    "morgan": "^1.9.1",
    "neo4j-driver": "^1.7.3",
    "parcel-bundler": "^1.12.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
