{
  "scripts": {
    "start": "./node_modules/nodemon/bin/nodemon.js",
    "clean": "rm -rf .cache build",
    "type-check": "tsc --noEmit"
  },
  "nodemonConfig": {
    "exec": "ts-node --files src/server/app.ts",
    "ext": "ts,js,ejs,css",
    "watch": [
      "src",
      "static"
    ],
    "ignore": [
      ".git",
      "node_modules"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
