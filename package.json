      "node_modules"
    ]
  },
  "dependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/morgan": "^1.7.35",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "neo4j-driver": "^1.7.3"
  },
  "devDependencies": {
    "nodemon": "^1.18.10",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.3.3333"
  }
}
